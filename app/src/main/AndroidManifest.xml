<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.bozsi.progression">

    <uses-permission android:name="android.permission.VIBRATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/appicon_fg"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_ic_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Back"
            android:label="Back"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".Biceps"
            android:label="Biceps"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".Chest"
            android:label="Chest"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".Triceps"
            android:label="Triceps"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".Frontlever"
            android:label="Frontlever"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".Planche"
            android:label="Planche"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".Routine"
            android:label="Routine"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".Timer"
            android:label="Timer"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".Plancheprogression"
            android:label="Plancheprogression"
            android:parentActivityName=".MainActivity"/>
        <activity
        android:name=".PowerPullup"
        android:label="Powertraining/Back/Pullup"/>
        <activity
            android:name=".PowerDip"
            android:label="Powertraining/Chest/Dip"/>
        <activity
            android:name=".PowerShoulderpress"
            android:label="Powertraining/Shoulder/Shoulderpress"/>
    </application>

</manifest>